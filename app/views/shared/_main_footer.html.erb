<div class="main-footer">
  <div class="pager-navi">
    <% if previous_object %>
      <div class ="previous">
        <%= link_to object_path(previous_object) do %>
          <span class="fa fa-caret-left"></span>
          <%= previous_object.title %>
        <% end %>
      </div>
    <% end %>
    <% if next_object %>
      <div class ="next">
        <%= link_to object_path(next_object) do %>
          <%= next_object.title %>
          <span class="fa fa-caret-right"></span>
        <% end %>
      </div>
    <% end %>
  </div>
</div>